{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#ffffff",
          "properties": {
            "style": "agnoster_left",
            "template": "<green>[</><lightGreen>{{ .Path }}</><green>]</> "
          },
          "style": "plain",
          "type": "path"
        },
        {
          "foreground": "#C2C206",
          "properties": {
            "template": "{{ .HEAD }} "
          },
          "style": "plain",
          "type": "git"
        },
        {
          "type": "kubectl",
          "style": "plain",
          "powerline_symbol": "\uE0B0",
          "foreground": "#000000",
          "background": "#ebcc34",
          "properties": {
            "template": " \uFD31 {{if .Namespace}}{{.Namespace}} :: {{end}}{{.Context}} "
          }
        },
        {
          "foreground": "#B5B50D",
          "properties": {
            "template": " \uf0e7 "
          },
          "style": "plain",
          "type": "root"
        },
        {
          "foreground": "#C94A16",
          "properties": {
            "template": "x{{ .Meaning }} "
          },
          "style": "plain",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#007ACC",
          "properties": {
            "template": "\ue602 "
          },
          "style": "plain",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 1
}
